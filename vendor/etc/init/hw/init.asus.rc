on boot


# adding big/little cpu affinity ---
    mkdir /dev/stune/nonprime-aff
    chown system system /dev/stune/nonprime-aff
    chmod 0664 /dev/stune/nonprime-aff/tasks

    mkdir /dev/cpuset/nonprime-aff
    write /dev/cpuset/nonprime-aff/cpus 0-5
    write /dev/cpuset/nonprime-aff/mems 0
    chown system system /dev/cpuset/nonprime-aff
    chown system system /dev/cpuset/nonprime-aff/tasks
    chmod 0660 /dev/cpuset/nonprime-aff/tasks
    
    mkdir /dev/stune/prime-aff
    chown system system /dev/stune/prime-aff
    chmod 0664 /dev/stune/prime-aff/tasks

    mkdir /dev/cpuset/prime-aff
    write /dev/cpuset/prime-aff/cpus 6-7
    write /dev/cpuset/prime-aff/mems 0
    chown system system /dev/cpuset/prime-aff
    chown system system /dev/cpuset/prime-aff/tasks
    chmod 0660 /dev/cpuset/prime-aff/tasks

    restorecon_recursive /dev/cpuset/prime-aff/
    restorecon_recursive /dev/cpuset/nonprime-aff/
    
##############################################################
    setprop vendor.asus.accy.fw_status  000000
    setprop vendor.asus.accy.fw_status2 000000
    setprop vendor.asus.accy.fw_status3 000000
    setprop vendor.asus.accy.fw_status4 000000
##############################################################

on charger
#set cos flag
    wait /sys/class/asuslib/enter_cos
    write  /sys/class/asuslib/enter_cos 1

#disable sepolicy 
    write  /sys/fs/selinux/log 1
    
#+++ ASUS_BSP : add for mount asdf in COS mode
    wait /dev/block/bootdevice/by-name/asdf
    mount ext4 /dev/block/bootdevice/by-name/asdf /asdf nosuid nodev noatime barrier=1
    chmod 0775 /asdf
    restorecon_recursive /asdf

    write /proc/asusevtlog  "ASDF: Check LastShutdown log."
    write /proc/asusdebug  get_asdf_log
    write /proc/asusevtlog "charger: enter charger mode"
    write /asdf/CHGLimit 0
    write /sys/fs/selinux/log 1

    start logd
    start logcat-charger
    start bat_bs
    start bat_safety
#--- ASUS_BSP : add for mount asdf in COS mode

# ASUS_BSP add cos_charger_limit +++
    chmod 0666 /dev/kmsg
    write /proc/asusevtlog  "ASUS RC : mount /batinfo"
	mkdir /batinfo
    wait /dev/block/bootdevice/by-name/batinfo
    exec /system/bin/e2fsck -p /dev/block/bootdevice/by-name/batinfo
    mount ext4 dev/block/bootdevice/by-name/batinfo /batinfo rw wait nosuid nodev noatime barrier=1
    chown system system /batinfo
    chown root system /batinfo/cos_charger_limit
    chmod 0777 /batinfo
    chmod 0777 /batinfo/cos_charger_limit
    restorecon_recursive /batinfo
	
	wait /batinfo/cos_charger_limit
    start cos_charger_limit_restore	
# ASUS_BSP add cos_charger_limit ---

